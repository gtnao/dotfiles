[ui]
  color = true
[init]
  defaultBranch = main
[fetch]
  prune = true
[pull]
  rebase = false
[push]
  default = current
[alias]
  br = branch
  cm = commit -m
  co = checkout
  rs = restore
  st = status
  sw = switch
  swc = switch -c
[ghq]
  root = ~/dev/src
